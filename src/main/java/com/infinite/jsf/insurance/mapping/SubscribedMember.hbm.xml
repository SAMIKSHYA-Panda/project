<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.infinite.jsf.insurance.model">

	<class name="SubscribedMember" table="subscribedmember">
		<id name="memberId" column="member_id">
			<generator class="assigned" />
		</id>
		<property name="hId" column="h_id" />
		<property name="subscriptionId" column="subscription_id" />
		<property name="fullName" column="full_name" />
		<property name="dob" column="dob" />
		<property name="age" column="age" />
		<property name="gender" column="gender" />
		<property name="relationWithProposer"
			column="relation_with_proposer" />
		<property name="aadharNo" column="aadhar_no" />

	</class>
</hibernate-mapping>